{
	"compilerOptions": {
		"target": "ES2022",
		"jsx": "react-jsx",
		"module": "ES2022",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"types": ["vite/client"],
		"baseUrl": "src",
		"paths": {
			"@pages": ["pages/index.ts"],
			"@hooks": ["hooks/index.ts"],
			"@components": ["components/index.ts"],
			"@utils": ["utils/index.ts"],
			"@service": ["service/index.ts"]
		}
	},
	"include": ["src"]
}
